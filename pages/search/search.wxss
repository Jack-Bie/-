.container{
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
  flex-wrap: wrap;
}
#search-box{
  display: flex;
  align-items: center;
  justify-content: space-around;
  width: 100%;
  height: 100rpx;

}
image{
  width: 60rpx;
  height: 60rpx;
}
.img{
  display: inline;
  width: 50rpx;
  height: 50rpx;
}
#search-text{
  display: flex;
  align-items: center;
  justify-content: space-around;
  background-color: #E5E5E5;
  border: 1px solid #E5E5E5;
  border-radius: 20rpx;
  box-shadow: 6rpx 6rpx 10rpx gray;
  width: 70%;
  height: 80%;
}
#search-text view{
  display: flex;
  align-items: center;
  width: 90%;
  height: 80%; 
  color: rgba(0, 0, 0, 0.5); 
  font-size: 32rpx;
}
.button{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 90rpx;
  height: 70rpx;
  border: 1rpx solid #e7d5ec;
  border-radius: 20rpx;
  box-shadow: 6rpx 6rpx 10rpx gray;
}
.search-history{
  display: flex;
  width: 100%;
  height: auto;
  flex-wrap: wrap;
}
.history{
  display: flex;
  width: 100%;
  height: auto;
  flex-wrap: wrap;
  /* flex-direction: column; */
  justify-content: center;
  align-items: center;
  margin-top: 50rpx;
}
.history .button{
  width: auto;
  margin-right: 20rpx;
  margin-top: 20rpx;
}
.search-text{
  width: 100%;
  height: 40rpx;
}